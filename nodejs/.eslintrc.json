/* https://eslint.org/docs/latest/use/configure/#configuring-rules */
{
  "extends": [
    "@rocketseat/eslint-config/node"
  ],
  "rules": {
    "camelcase": "off",
    "no-useless-constructor": "off",
    "object-curly-spacing": "off",
    "space-infix-ops": "off",
    "brace-style": ["error", "1tbs"]
  }
}